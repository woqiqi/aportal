                "breakpoint": 1200,
                "settings": {
                  "slidesToShow": 3
                }
              }, {
                "breakpoint": 992,
                "settings": {
                  "slidesToShow": 2
                }
              }, {
                "breakpoint": 768,
                "settings": {
                  "slidesToShow": 2
                }
